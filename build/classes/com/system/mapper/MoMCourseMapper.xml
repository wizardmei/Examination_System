<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.system.mapper.MoMCourseMapper">
	<insert id="addInfo">
		insert into momcourse values('${crs_code}','${userid}','${crs_attribute}')
	</insert>	
	<select id="electivecourses" resultType="com.system.po.MoMCourse">
		select * from momcourse where userid = ${userid} and crs_attribute = '${crs_attribute}'
	</select>
</mapper>